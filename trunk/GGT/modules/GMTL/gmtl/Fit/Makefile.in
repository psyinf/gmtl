default: all

# Include common definitions.
include @topdir@/make.defs.mk

srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
BSYS_ROOT	= $(top_srcdir)
BSYS_SRCDIR	= @BSYS_SRC_ROOT@/Pool
WORKDIR		= $(topdir)/Pool

# Search path for source code.
vpath
vpath %.cpp @srcdir@ .

# Prefix for recursive stuff.
DIRPRFX		= GMTL/Fit/

# Subdirectories to compile.
#SUBDIR		= Fit

SRCS		:=  GaussPointsFit.cpp
			   	

# Include only stuff for building the local objects
include $(MKPATH)/bsys.obj.mk

# Include everything needed for building object files and recursing.
#include $(MKPATH)/bsys.obj-subdir.mk

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
CLEAN_FILES	+= $(OBJECTS)

# -----------------------------------------------------------------------------
# Include dependencies generated automatically.
# -----------------------------------------------------------------------------
ifndef DO_CLEANDEPEND
ifndef BEFOREBUILD
    include $(DEPEND_FILES)
endif
endif
